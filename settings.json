{
	/****************** TELEMETRY ******************/
	"telemetry.telemetryLevel": "off",
	"redhat.telemetry.enabled": false,
	"rest-client.enableTelemetry": false,
	/****************** TELEMETRY ******************/

	/********************** SECURITY **********************/
	"security.workspace.trust.untrustedFiles": "newWindow",
	/********************** SECURITY **********************/

	/******************* EXPLORER *******************/
	"explorer.confirmDelete": true,
	/******************* EXPLORER *******************/

	/******************** FILES ********************/
	"files.autoSave": "onFocusChange",
	"files.associations": {
		"*.html": "html",
		"*.twig": "html"
	},
	/******************** FILES ********************/

	/****************** BREADCRUMBS ******************/
	"breadcrumbs.enabled": true,
	/****************** BREADCRUMBS ******************/

	/****************** WORKBENCH ******************/
	"workbench.activityBar.location": "top",
	"workbench.statusBar.visible": true,
	"workbench.enableExperiments": false,
	"workbench.iconTheme": "vscode-icons",
	"workbench.colorTheme": "GitHub Dark",
	"workbench.colorCustomizations": {
		"editorRuler.foreground": "#71797E"
	},
	/****************** WORKBENCH ******************/

	/******************** EDITOR ********************/
	"editor.minimap.enabled": true,
	"editor.rulers": [80],
	"editor.quickSuggestions": {
		"comments": true,
		"strings": true,
		"other": true
	},
	"editor.suggestSelection": "first",
	"editor.wordWrap": "on",
	"editor.formatOnSave": true,
	"editor.fontSize": 15,
	"editor.bracketPairColorization.enabled": true,
	"editor.unicodeHighlight.nonBasicASCII": false,
	"editor.linkedEditing": true,
	/******************** EDITOR ********************/

	/********************* EMMET *********************/
	"emmet.triggerExpansionOnTab": true,
	"emmet.includeLanguages": {
		"javascript": "javascriptreact",
		"typescript": "typescriptreact",
		"php": "html",
		"twig": "html"
	},
	/********************* EMMET *********************/

	/********************** GIT **********************/
	"git.autofetch": true,
	"git.suggestSmartCommit": false,
	"git.confirmSync": false,
	"git.ignoreRebaseWarning": true,
	/********************** GIT **********************/

	/****************** DIFF EDITOR ******************/
	"diffEditor.ignoreTrimWhitespace": false,
	/****************** DIFF EDITOR ******************/

	/******************** TERMINAL ********************/
	"terminal.integrated.fontFamily": "monospace",
	"terminal.integrated.fontSize": 15,
	/******************** TERMINAL ********************/

	/************** LIVESERVER EXTENSION **************/
	"liveServer.settings.donotShowInfoMsg": true,
	/************** LIVESERVER EXTENSION **************/

	/*************** TODO TREE EXTENSION ***************/
	"todo-tree.general.tags": [
		"BUG",
		"HACK",
		"FIXME",
		"TODO",
		"XXX",
		"[ ]",
		"[x]"
	],
	"todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
	/*************** TODO TREE EXTENSION ***************/

	/*********** CODE SPELL CHECKER EXTENSION ***********/
	"cSpell.language": "ru,en,en-GB,en-US,ru-RU,lorem",
	"cSpell.ignorePaths": [
		"**/settings.json",
		"package-lock.json",
		"node_modules",
		"vendor",
		".git",
		".vscode"
	],
	"cSpell.enabled": true,
	"cSpell.userWords": [],
	/*********** CODE SPELL CHECKER EXTENSION ***********/

	/************* AUTO CLOSE TAG EXTENSION *************/
	"auto-close-tag.activationOnLanguage": [
		"html",
		"xml",
		"php",
		"blade",
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact",
		"plaintext",
		"markdown"
	],
	/************* AUTO CLOSE TAG EXTENSION *************/

	/*********** MARKDOWN ALL IN ONE EXTENSION ***********/
	"markdown.extension.toc.updateOnSave": false,
	/*********** MARKDOWN ALL IN ONE EXTENSION ***********/

	/******************* JS/JSX/TS/TSX *******************/
	"[javascript][javascriptreact][typescript][typescriptreact]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"javascript.updateImportsOnFileMove.enabled": "always",
	"typescript.updateImportsOnFileMove.enabled": "always",
	/******************* JS/JSX/TS/TSX *******************/

	/*********************** XML ***********************/
	"[xml]": {
		"editor.defaultFormatter": "redhat.vscode-xml"
	},
	/*********************** XML ***********************/

	/********************* HTML/CSS *********************/
	"[html][css]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	/********************* HTML/CSS *********************/

	/******************** JSON/JSONC ********************/
	"[json][jsonc]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	/******************** JSON/JSONC ********************/

	/*********************** PHP ***********************/
	"[php]": {
		"editor.defaultFormatter": "junstyle.php-cs-fixer"
	},
	"php.problems.exclude": {
		"vendor/": true
	},
	/*********************** PHP ***********************/

	/************** PHP CS FIXER EXTENSION **************/
	"php-cs-fixer.rules": "@PhpCsFixer",
	"php-cs-fixer.config": ".php-cs-fixer.php;.php-cs-fixer.dist.php",
	"php-cs-fixer.pathMode": "override",
	"php-cs-fixer.ignorePHPVersion": false,
	"php-cs-fixer.onsave": true,
	"php-cs-fixer.autoFixByBracket": false,
	"php-cs-fixer.autoFixBySemicolon": false,
	"php-cs-fixer.formatHtml": false,
	"php-cs-fixer.executablePath": "${extensionPath}/php-cs-fixer.phar",
	"php-cs-fixer.lastDownload": 0,
	/************** PHP CS FIXER EXTENSION **************/

	/************ PHP INTELEPHENSE EXTENSION ************/
	"intelephense.telemetry.enabled": false,
	"intelephense.files.exclude": [
		"**/.git/**",
		"**/.svn/**",
		"**/.hg/**",
		"**/CVS/**",
		"**/.DS_Store/**",
		"**/node_modules/**",
		"**/bower_components/**",
		"**/vendor/**/{Test,test,Tests,tests}/**/*Test.php"
	],
	"intelephense.files.associations": ["*.php", "*.phtml"],
	/************ PHP INTELEPHENSE EXTENSION ************/

	/*********************** BLADE ***********************/
	"[blade]": {
		"editor.defaultFormatter": "shufo.vscode-blade-formatter"
	},
	/*********************** BLADE ***********************/

	/**************** SQL TOOLS EXTENSION ****************/
	"sqltools.useNodeRuntime": true,
	/**************** SQL TOOLS EXTENSION ****************/

	/************** CONTAINER TOOLS EXTENSION **************/
	"containers.containerClient": "com.microsoft.visualstudio.containers.docker",
	"containers.orchestratorClient": "com.microsoft.visualstudio.orchestrators.dockercompose",
	/************** CONTAINER TOOLS EXTENSION **************/

	/***************** KUBERNETES EXTENSION *****************/
	"vs-kubernetes": {
		"vs-kubernetes.suppress-helm-not-found-alerts": true
	},
	/***************** KUBERNETES EXTENSION *****************/

	/****************** PYTHON ******************/
	"[python]": {
		"editor.defaultFormatter": "ms-python.vscode-pylance"
	},
	"python.analysis.ignore": ["**/vendor/**/*.py"],
	"python.languageServer": "Pylance",
	"python.analysis.indexing": false,
	/****************** PYTHON ******************/

	/******************** GO ********************/
	"[go]": {
		"editor.defaultFormatter": "gofenix.go-lines",
		"editor.insertSpaces": true,
		"editor.codeActionsOnSave": {
			"source.organizeImports": "explicit"
		}
	},
	"[go.mod]": {
		"editor.defaultFormatter": "golang.go"
	},
	"go.lintOnSave": "package",
	"go.useLanguageServer": true,
	"gopls": {
		"ui.semanticTokens": true
	},
	"go.toolsManagement.autoUpdate": false,
	/******************** GO ********************/

	/******************* RUST *******************/
	"[rust]": {
		"editor.defaultFormatter": "rust-lang.rust-analyzer"
	},
	"rust-analyzer.diagnostics.disabled": ["unresolved-proc-macro"],
	/******************* RUST *******************/

	/**************** EXTENSIONS ****************/
	"extensions.autoCheckUpdates": false,
	"extensions.autoUpdate": false,
	"update.showReleaseNotes": false,
	"update.mode": "none"
	/**************** EXTENSIONS ****************/
}
